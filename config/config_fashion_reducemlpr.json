{
    "dataset_type": "fashion",
    "dataset_path": "./dataset/fashion/",
    "enable_line_nortify": true,

    "name": "fashion_reducemlpr",
    "n_gpu": 2,
    "steps": 500000,
    "batch_size": 4,
    "milestones": [250000, 400000],
    "learning_rate_G": 1e-4,
    "learning_rate_D": 1e-4,
    "betas": [0.5, 0.999], 

    "coef_l1": 2.5,
    "coef_perc": 0.25,
    "coef_style": 250,
    "coef_gp": 10,
    
    "print_freq": 100,
    "eval_freq": 1000,
    "val_step": 100,
    "pretrained_path": null,
    "bestpoint_metrics": null,

    "input_image_resolution": [256, 176],
    "input_pose_resolution": [256, 176],
    "random_rotate_range": [0, 0],
    "use_random_horizontal_flip": false,

    "color_channels": 3,
    "pose_channels": 18,

    "level": 5,
    "n_cnn_trans": 3,
    "n_at_trans": 3, 
    "dims": [64, 64, 64, 128, 128],

    "feature_extraction": {
        "depths": [2, 2, 4, 4, 4],
        "ksizes": [7, 7, 5, 3, 3]
    },

    "cnn_trans": {
        "depths": [6, 4, 4],
        "ksizes": [5, 7, 7]
    }, 

    "to_stat": {
        "ksizes": [5, 7, 7]
    },

    "at_trans": {
        "heads": [8, 8, 8], 
        "mlp_ratios": [1, 1, 1],
        "drops": [0.0, 0.0, 0.0], 
        "cpe_ksizes": [3, 5, 7], 
        "encoder_depths": [6, 4, 4], 
        "decoder_depths": [6, 4, 4]
    }
}